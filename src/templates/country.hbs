{{#each this}}
<h2 class="country-name">{{name}}</h2>
<div class="wrapper">
    <div class="country-info">
        <p>Capital: {{capital}}</p>
        <p>Poppulation: {{population}}</p>
        <p>Languages:</p>
        <ul>
            {{#each languages}}
            <li>{{this.name}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="country-flag">
        <img src="{{flag}}" alt="flag">
    </div>
</div>
{{/each}}